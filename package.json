{
  "name": "notist",
  "productName": "Notist",
  "private": true,
  "version": "0.0.0",
  "main": "dist/main/main/main.js",
  "scripts": {
    "copy-assets": "cp -r node_modules/@excalidraw/excalidraw/dist/excalidraw-assets dist",
    "dev": "yarn copy-assets && concurrently \"tsc --project tsconfig.main.json --watch\" \"vite\"",
    "build": "yarn copy-assets && concurrently \"tsc --project tsconfig.main.json\" \"vite build\"",
    "start": "electron-forge start",
    "package": "yarn build && electron-forge package",
    "make": "yarn build && electron-forge make",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.15.2",
    "@js-joda/core": "^5.4.2",
    "@js-joda/locale": "^4.8.6",
    "@js-joda/timezone": "^2.15.0",
    "@k0michi/now": "^0.0.1",
    "@tabler/icons": "^1.111.0",
    "charset": "^1.0.1",
    "cldr-data": "^36.0.1",
    "cldrjs": "^0.5.5",
    "content-type": "^1.0.4",
    "electron-squirrel-startup": "^1.0.0",
    "immer": "^9.0.15",
    "jsdom": "^20.0.2",
    "katex": "^0.16.3",
    "kyoka": "^0.0.3",
    "mime": "^3.0.0",
    "modern-normalize": "^1.1.0",
    "node-fetch": "2",
    "react": "^18.2.0",
    "react-contenteditable": "^3.3.7",
    "react-dom": "^18.2.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.65",
    "@electron-forge/maker-deb": "^6.0.0-beta.65",
    "@electron-forge/maker-rpm": "^6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.65",
    "@electron-forge/maker-zip": "^6.0.0-beta.65",
    "@types/charset": "^1.0.2",
    "@types/content-type": "^1.1.5",
    "@types/electron": "^1.6.10",
    "@types/jsdom": "^20.0.1",
    "@types/katex": "^0.14.0",
    "@types/mime": "^3.0.1",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^2.0.0",
    "concurrently": "^7.3.0",
    "electron": "^22.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "assets/notist.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "vite_react_boilerplate"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
